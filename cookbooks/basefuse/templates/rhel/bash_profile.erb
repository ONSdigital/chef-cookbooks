# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
. ~/.bashrc
fi

# User specific environment and startup programs
ulimit -u <%= @process_limit %>

PATH=/usr/local/bin:/bin:/usr/bin:/usr/local/sbin:/usr/sbin:/sbin:<%= @fuse_bin_dir %>:<%= @home_dir %>/bin

export PATH
export JAVA_HOME=/usr/lib/jvm/jre
